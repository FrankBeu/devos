{
  ### https://github.com/Base24/base24-framer-scheme
  custom-base24-framer = {
    slug   = "framer";
    name   = "Framer";
    author = "Base24";
    colors = {
      base00 = "111111"; ### "#111111" Default Background                                                 ##
      base01 = "141414"; ### "#141414" Lighter Background (Used for status bars)
      base02 = "414141"; ### "#414141" Selection Background
      base03 = "636363"; ### "#636363" Comments, Invisibles, Line Highlighting
      base04 = "868686"; ### "#868686" Dark Foreground (Used for status bars)
      base05 = "a9a9a9"; ### "#a9a9a9" Default Foreground, Caret, Delimiters, Operators
      base06 = "cccccc"; ### "#cccccc" Light Foreground (Not often used)
      base07 = "ffffff"; ### "#ffffff" Light Background (Not often used)
      base08 = "ff5555"; ### "#ff5555" Variables, XML Tags, Markup Link Text, Markup Lists, Diff Deleted  #
      base09 = "ffcc33"; ### "#ffcc33" Integers, Boolean, Constants, XML Attributes, Markup Link Url
      base0A = "33bbff"; ### "#33bbff" Classes, Markup Bold, Search Text Background                       #
      base0B = "98ec65"; ### "#98ec65" Strings, Inherited Class, Markup Code, Diff Inserted               #
      base0C = "88ddff"; ### "#88ddff" Support, Regular Expressions, Escape Characters, Markup Quotes     #
      base0D = "00aaff"; ### "#00aaff" Functions, Methods, Attribute IDs, Headings                        #
      base0E = "aa88ff"; ### "#aa88ff" Keywords, Storage, Selector, Markup Italic, Diff Changed           #
      base0F = "7f2a2a"; ### "#7f2a2a" Deprecated, Opening/Closing Embedded Language Tags, e.g. <?php ?>
      base10 = "2b2b2b"; ### "#2b2b2b" base00 darker
      base11 = "151515"; ### "#151515" base00 darker
      base12 = "ff8888"; ### "#ff8888" base08 lighter
      base13 = "ffd966"; ### "#ffd966" base0A lighter
      base14 = "b6f292"; ### "#b6f292" base0B lighter
      base15 = "bbecff"; ### "#bbecff" base0C lighter
      base16 = "33bbff"; ### "#33bbff" base0D lighter
      base17 = "cebbff"; ### "#cebbff" base0E lighter
    };
  };
}
